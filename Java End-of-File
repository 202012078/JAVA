import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        Scanner a = new Scanner(System.in);
        String s;
        for(int i = 1; a.hasNext() ;i++)
        {
            s = a.nextLine();
            System.out.println(i + " " + s);
        }
    }
}
